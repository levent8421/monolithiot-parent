<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.monolithiot.iot.notification.entity.Notification">
    <sql id="columns">
        n.id          n_id,
        n.trace_no    n_trace_no,
        n.type        n_type,
        n.target      n_target,
        n.user_id     n_user_id,
        n.code        n_code,
        n.extend_data n_extend_data,
        n.expire_in   n_expire_in,
        n.create_time n_create_time,
        n.update_time n_update_time,
        n.deleted     n_deleted
    </sql>
    <sql id="select">
        select
        <include refid="columns"/>
        from t_notification as n
        where n.deleted = false
    </sql>
    <resultMap id="rm_base" type="com.monolithiot.iot.notification.entity.Notification">
        <id column="n_id" property="id"/>
        <result column="n_trace_no" property="traceNo"/>
        <result column="n_type" property="type"/>
        <result column="n_target" property="target"/>
        <result column="n_user_id" property="userId"/>
        <result column="n_code" property="code"/>
        <result column="n_extend_data" property="extendData"/>
        <result column="n_expire_in" property="expireIn"/>
        <result column="n_create_time" property="createTime"/>
        <result column="n_update_time" property="updateTime"/>
        <result column="n_deleted" property="deleted"/>
    </resultMap>
</mapper>